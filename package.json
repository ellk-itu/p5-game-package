    "@types/jest": "^30.0.0",
    "@types/p5": "^1.7.7",
    "babel-jest": "^30.2.0",
    "eslint": "^9.39.2",
    "globals": "^16.5.0",
    "jest": "^30.2.0",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.50.0"
  },
  "dependencies": {
    "@types/node": "^25.0.2",
    "p5": "^2.1.1"
  },
  "files": [
