  },
  "license": "ISC",
  "author": "Vandmand",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "test": "jest",
    "build": "npx tsc",
    "link": "npm link && cd ./test/ && npm link p5-game",
    "unlink": "cd ./test/ && npm unlink p5-game",
    "link-remove": "npm rm -g p5-game",
    "link-update": "npm run unlink && npm run link-remove && npm run build && npm run link",
    "l": "npm run link",
    "ul": "npm run unlink",
    "lr": "npm run link-remove",
    "lu": "npm run link-update"
  },
  "devDependencies": {
    "@babel/core": "^7.28.5",
