{
  "name": "p5-game",
  "version": "1.0.0",
  "description": "A game framework for p5 in javascript",
  "keywords": [
    "game",
    "framework",
    "p5"
  ],
  "homepage": "https://github.com/Vandmand/p5game#readme",
  "bugs": {
